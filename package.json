{
  "name": "lees-friends-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "NODE_ENV=development nodemon ./bin/www",
    "grunt:dev": "grunt dev",
    "grunt:staging": "grunt staging",
    "grunt:prod": "grunt prod",
    "client:staging": "cd client/ && npm run build:prod && cd ..",
    "client:prod": "cd client/ && npm run build:prod & cd ..",
    "build:staging": "npm run client:staging && npm run grunt:staging",
    "build:prod": "npm run client:prod && npm run grunt:prod",
    "deploy:staging": "npm run build:staging && gcloud app deploy --project=lees-friends-staging --verbosity=info --quiet",
    "deploy:prod": "npm run build:prod && gcloud app deploy --project=lees-friends --verbosity=info"
  },
  "dependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/jsonwebtoken": "^7.1.33",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "helmet": "^3.1.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^7.2.1",
    "lodash": "^4.17.2",
    "mongodb": "^2.2.16",
    "morgan": "^1.7.0",
    "nedb": "^1.8.0",
    "q": "^1.4.1",
    "rxjs": "^5.0.1",
    "serve-favicon": "^2.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/cookie-parser": "^1.3.30",
    "@types/cors": "0.0.33",
    "@types/helmet": "0.0.33",
    "@types/method-override": "0.0.29",
    "@types/morgan": "^1.7.32",
    "@types/nedb": "0.0.31",
    "@types/serve-favicon": "^2.2.28",
    "grunt": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-set-env": "^0.1.3",
    "grunt-ts": "^6.0.0-beta.3",
    "method-override": "^2.3.7",
    "nodemon": "^1.11.0",
    "typescript": "^2.0.10"
  }
}
